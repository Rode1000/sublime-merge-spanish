	{
		"caption": "Editar",
		"mnemonic": "E",
		"id": "edit",
		"children":
		[
			{ "command": "undo", "caption": "Deshacer", "mnemonic": "D" },
			{ "command": "redo_or_repeat", "caption": "Rehacer o repetir", "mnemonic": "R" },
