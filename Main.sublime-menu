				"caption": "Editar .gitattributes…",
				"command": "open_in_editor",
				"args": {
					"path": "$working_dir/.gitattributes", "line": "$line", "col": "$col",
					"default": "# Specify filepatterns you want to assign special attributes.\n\n"
				}
			},
			{
				"caption": "Editar .gitignore…",
				"command": "open_in_editor",
				"args": {
					"path": "$working_dir/.gitignore", "line": "$line", "col": "$col",
					"default": "# Specify filepatterns you want git to ignore.\n\n"
				}
			},
			{
				"caption": "Caché de credenciales",
				"platform": "!Windows",
				"children":
				[
					{
						"command": "git_config",
						"caption": "No cachear",
						"args":
						{
							"key": "credential.helper",
							"value": "cache --timeout=0",
							"is_default_value": true
						},
						"checkbox": true
					},
					{
						"command": "git_config",
						"caption": "Cachear 10 minutos",
						"args":
						{
							"key": "credential.helper",
							"value": "cache --timeout=600"
						},
						"checkbox": true
					},
					{
						"command": "git_config",
						"caption": "Cachear 1 hora",
						"args":
						{
							"key": "credential.helper",
							"value": "cache --timeout=3600"
						},
						"checkbox": true
					},
					{
						"command": "git_config",
						"caption": "Cachear 1 día",
						"args":
						{
							"key": "credential.helper",
							"value": "cache --timeout=86400"
						},
						"checkbox": true
					},
					{
						"command": "git_config",
						"caption": "Cachear 1 semana",
						"args":
						{
							"key": "credential.helper",
							"value": "cache --timeout=604800"
						},
						"checkbox": true
					}
				]
			},
			{ "caption": "-" },
