			{ "command": "select_all", "caption": "Seleccionar todo" },
			{ "caption": "-" },
			{ "command": "wrap_lines", "caption": "Ajuste de línea" },
			{ "caption": "-", "id": "end" }
		]
	},
	{
		"caption": "Repositorio",
		"mnemonic": "R",
		"id": "repository",
		"children":
		[
			{ "command": "git_undo", "caption": "Deshacer Git", "mnemonic": "D" },
		    { "command": "git_redo", "caption": "Rehacer Git", "mnemonic": "R" },
			{ "caption": "-" },
			{ "command": "stash", "caption": "Guardar en Stash (Resguardo)" },
			{ "command": "show_command_palette", "args": { "command": "stash" }, "caption": "Guardar en Stash con opciones…" },
			{ "command": "pop_stash", "caption": "Recuperar de Stash (Pop)" },
			{ "caption": "-" },
			{ "command": "apply_patch", "caption": "Aplicar parche…" },
			{ "caption": "-" },
			{ "command": "show_command_palette", "args": { "command": "pull" }, "caption": "Pull (Traer)…" },
			{
				"caption": "Push (Enviar)…",
				"command": "show_command_palette",
				"args": { "command": "push", "args": {"prompt": true} }
			},
			{ "caption": "-" },
			{
				"caption": "Cambiar rama (Checkout)…",
				"command": "show_command_palette",
				"args": { "command": "checkout_branch" }
			},
			{
				"caption": "Cambiar rama remota (Checkout)…",
				"command": "show_command_palette",
				"args":
				{
					"command": "checkout_branch",
					"args": { "local_refs": false, "remote_refs": true }
				}
			},
			{
				"caption": "Crear rama…",
				"command": "show_command_palette",
				"args":
				{
					"command": "create_branch"
				}
			},
			{
				"caption": "Crear rama desde rama remota…",
				"command": "show_command_palette",
				"args":
				{
					"command": "create_branch_from_remote_branch"
				}
			},
			{
				"caption": "Renombrar rama…",
				"command": "show_command_palette",
				"args":
				{
					"command": "rename_branch"
				}
			},
			{
				"caption": "Eliminar rama…",
				"command": "show_command_palette",
				"args":
				{
					"command": "delete_branch",
					"args": {"prompt": false}
				}
			},
			{ "caption": "-" },
			{
				"caption": "Fusionar rama (Merge)…",
				"command": "show_command_palette",
				"args":
				{
					"command": "merge_branch"
				}
			},
			{
				"caption": "Rebasar rama (Rebase)…",
				"command": "show_command_palette",
				"args":
				{
					"command": "rebase_branch"
				}
			},
			{ "caption": "-" },
			{ "command": "open_dir", "args": {"dir": "$working_dir"}, "caption": "Abrir carpeta contenedora…" },
			{ "command": "open_dir_in_sublime_text", "args": {"dir": "$working_dir"}, "caption": "Abrir en Sublime Text…" },
			{ "caption": "-", "id": "end" }
